[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[alias]
	a = add
	aa = add --all
	b = branch
	ba = branch -a
	bd = branch -d
	bdf = branch -D
	br = branch --remote
	c = commit -v
	cb = checkout -b
	cm = checkout main
	cmsg = commit -m
	co = checkout
	d = diff
	dc = diff --cached
	f = fetch
	fa = fetch --all --prune
	l = pull
	loga = log --oneline --decorate --graph --all
	logo = log --oneline --decorate --graph
	m = merge
	p = push
	s = status --find-renames --show-stash
	sm = submodule
